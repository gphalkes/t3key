_kp_kx_nokx {
up = "\e[A"
left = "\e[D"
down = "\e[B"
right = "\e[C"
up+c = "\eOa"
left+c = "\eOd"
down+c = "\eOb"
right+c = "\eOc"
up+m = "\e\e[A"
left+m = "\e\e[D"
down+m = "\e\e[B"
right+m = "\e\e[C"
up+s = "\e[a"
left+s = "\e[d"
down+s = "\e[b"
right+s = "\e[c"
up+cm = "\e\eOa"
left+cm = "\e\eOd"
down+cm = "\e\eOb"
right+cm = "\e\eOc"
# up+cs = up+s
# left+cs = left+s
# down+cs = down+s
# right+cs = right+s
up+ms = "\e\e[a"
left+ms = "\e\e[d"
down+ms = "\e\e[b"
right+ms = "\e\e[c"
# up+cms = up+ms
# left+cms = left+ms
# down+cms = down+ms
# right+cms = right+ms
}

