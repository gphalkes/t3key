%best kp_kx

# In nokx mode, the keypad can switch to application keypad mode by pressing
# numlock twice. However, without switching to application keypad mode the
# keypad produces the same keys as the normal keys. So by including the
# application keypad in the nokx map (which in this case does produce
# collisions) the nokx map will always work. However, it's best to simply
# tell Eterm that it should use the application keypad.


nokx {
%enter = "\e[?1;66l"
%include _kp_kx_kx_nokx
%include _kp_kx_nokx
%include _arrow_kx_kp_kx_kx_nokx
}

kx {
%enter = "\e[?1;66h"
%leave = "\e[?1;66l"
%include _kp_kx_kx_nokx
%include _arrow_kx_kp_kx_kx_nokx
%include _arrow_kx_kx
}

kp_kx {
%enter = "\e[?1l\e[?66h"
%leave = "\e[?1;66l"
%include _kp_kx_kx_nokx
%include _kp_kx_nokx
%include _arrow_kx_kp_kx_kx_nokx
}

arrow_kx {
%enter = "\e[?1h\e?66l"
%leave = "\e[?1;66l"
%include _arrow_kx_kp_kx_kx_nokx
%include _arrow_kx_kx
# kp_home = home
